{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "command": "cargo run",
      "problemMatcher": [
        "$rustc"
      ],
      "label": "rust: cargo run",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "runOptions": {
        "instanceLimit": 1,
        "reevaluateOnRerun": true,
        "runOn": "default"
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": true,
        "panel": "shared",
        "showReuseMessage": false,
        "clear": true,
        "close": true,
      },
    }
  ]
}